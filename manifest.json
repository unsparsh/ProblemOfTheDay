{
    "manifest_version": 3,
    "name": "Problem of the Day",
    "version": "1.0",
    "description": "Displays Problem of the Day and a random question from GFG and LeetCode.",
    "permissions": ["activeTab", "https://*.geeksforgeeks.org/*", "https://*.leetcode.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }